{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "memento",
  "main": "@tanstack/react-start/server-entry",
  "compatibility_date": "2024-11-17",
  "compatibility_flags": ["nodejs_compat"],

  // Production environment (default)
  "vars": {
    "VITE_API_URL": "https://memento-api.oddlab.cc",
    "VITE_WS_URL": "wss://memento-api.oddlab.cc"
  },

  // Beta environment
  "env": {
    "beta": {
      "name": "memento-beta",
      "vars": {
        "VITE_API_URL": "https://beta.memento-api.oddlab.cc",
        "VITE_WS_URL": "wss://beta.memento-api.oddlab.cc"
      }
    }
  }
}
